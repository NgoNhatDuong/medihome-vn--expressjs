{
    "name": "medihome-vn--expressjs",
    "version": "1.0.0",
    "description": "Medihome",
    "main": "dist/index.js",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "install-primary": "npm i dotenv express helmet jsonwebtoken bcrypt mysql nodemailer node-schedule && npm i -D @types/node @types/express @types/jsonwebtoken @types/bcrypt @types/nodemailer @types/node-schedule",
        "install-env": "npm i -D nodemon typescript@4.5.5 ts-node eslint prettier @typescript-eslint/eslint-plugin @typescript-eslint/parser eslint-config-prettier eslint-plugin-prettier eslint-plugin-import eslint-config-airbnb-typescript",
        "install-other": "npm i typeorm graphql@15.8.0 type-graphql class-validator mongodb reflect-metadata winston winston-daily-rotate-file morgan && npm i -D @types/morgan",
        "watch-tsc": "tsc --watch --project tsconfig.json",
        "watch-dist": "nodemon --delay 1.5 --watch dist --exec \"node dist/index.js\"",
        "watch-src": "nodemon --delay 1.5 --ext ts --watch src --exec \"npx tsc && node dist/index.js\"",
        "watch-tsnode": "nodemon --delay 1.5 --ext ts --watch src src/index.ts",
        "build": "eslint . --ext .ts --fix && tsc --build --clean && tsc",
        "dist": "node dist/index.js",
        "start": "npm run build && node dist/index.js",
        "stop": "taskkill /f /im node.exe",
        "test": "nodemon --delay 1.5 --watch src src/test.ts"
    },
    "keywords": [],
    "author": "duycode.com",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.0.1",
        "class-validator": "^0.13.2",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "graphql": "^15.8.0",
        "helmet": "^5.0.2",
        "jsonwebtoken": "^8.5.1",
        "mongodb": "^4.5.0",
        "morgan": "^1.10.0",
        "mysql": "^2.18.1",
        "node-schedule": "^2.1.0",
        "nodemailer": "^6.7.3",
        "reflect-metadata": "^0.1.13",
        "type-graphql": "^1.1.1",
        "typeorm": "^0.3.6",
        "winston": "^3.7.2",
        "winston-daily-rotate-file": "^4.6.1"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/morgan": "^1.9.3",
        "@types/node": "^17.0.25",
        "@types/node-schedule": "^1.3.2",
        "@types/nodemailer": "^6.4.4",
        "@typescript-eslint/eslint-plugin": "^5.20.0",
        "@typescript-eslint/parser": "^5.20.0",
        "eslint": "^8.13.0",
        "eslint-config-airbnb-typescript": "^16.2.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.0.0",
        "nodemon": "^2.0.15",
        "prettier": "^2.6.2",
        "ts-node": "^10.7.0",
        "typescript": "^4.5.5"
    }
}
